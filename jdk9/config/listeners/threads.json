{
  "namespace": {
    "-id": "openjdk",
    "-system": "securejvm",
    "-org": "org.mearvk.securejvm",
    "module": {
      "-type": "security",
      "-name": "config.listeners.thread",
      "setting": {
        "-id": "DefaultThreadListener",
        "parameter": {
          "-id": "0",
          "-name": "DefaultThreadListener",
          "#text": "java.lang.system.listeners.DefaultThreadListener"
        },
        "value": [
          {
            "-id": "1",
            "-name": "DefaultInstanceName",
            "#text": "DefaultThreadListener"
          },
          {
            "-id": "2",
            "-name": "DefaultRMIName",
            "#text": "jdk9/dragon/DefaultThreadListener"
          },
          {
            "-id": "3",
            "-name": "ExceptionHandler",
            "-class": "org.securejvm.DefaultExceptionHandler",
            "-out": "${PROJECT}/${MODULE}/exceptions/"
          }
        ]
      }
    }
  }
}
